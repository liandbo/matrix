void checkLower(int n, int array[n][n])
{
    int i,j;
    for(i=0;i<n;i++)
       {
		 for(j=n-1;j>i;j--)
           { if (array[i][j] != 0)
            {
                printf("Not lower triagonal matrix\n");
                return;
            }
        }
    }
    printf ("Lower triagonal matrix\n");
}
